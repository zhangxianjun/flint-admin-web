{
  "proxy": {
    "/flint": {
    "enable": true,
    "target": "http://localhost:18080",
    "pathRewrite": {
      "^/flint": "/" 
    }
  }
},
  "plugins": [
    [
      "build-plugin-fusion",
      {
        "themePackage": "@alifd/theme-design-pro"
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ]
  ]
}
